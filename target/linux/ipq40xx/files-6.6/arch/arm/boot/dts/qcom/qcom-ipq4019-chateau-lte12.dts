#include "qcom-ipq4019.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/leds/common.h>
#include <dt-bindings/soc/qcom,tcsr.h>

/ {
	model = "MikroTik Chateau LTE12";
	compatible = "mikrotik,chateau-lte12";

	memory {
		device_type = "memory";
		reg = <0x80000000 0x10000000>;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};

	aliases {
		led-boot = &led_status_blue;
		led-failsafe = &led_status_red;
		led-running = &led_status_blue;
		led-upgrade = &led_status_red;
	};

	soc {
		counter@4a1000 {
			compatible = "qcom,qca-gcnt";
			reg = <0x4a1000 0x4>;
		};

		tcsr@1949000 {
			compatible = "qcom,tcsr";
			reg = <0x1949000 0x100>;
			qcom,wifi_glb_cfg = <TCSR_WIFI_GLB_CFG>;
		};

		tcsr@194b000 {
			/* select hostmode */
			compatible = "qcom,tcsr";
			reg = <0x194b000 0x100>;
			qcom,usb-hsphy-mode-select = <TCSR_USB_HSPHY_HOST_MODE>;
			status = "okay";
		};

		ess_tcsr@1953000 {
			compatible = "qcom,tcsr";
			reg = <0x1953000 0x1000>;
			qcom,ess-interface-select = <TCSR_ESS_PSGMII>;
		};

		tcsr@1957000 {
			compatible = "qcom,tcsr";
			reg = <0x1957000 0x100>;
			qcom,wifi_noc_memtype_m0_m2 = <TCSR_WIFI_NOC_MEMTYPE_M0_M2>;
		};
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			gpios = <&tlmm 5 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};

	leds {
		compatible = "gpio-leds";

		led_status_blue: status-blue {
			function = LED_FUNCTION_STATUS;
			color = <LED_COLOR_ID_BLUE>;
			gpios = <&tlmm 0 GPIO_ACTIVE_HIGH>;
		};

		led_status_red: status-red {
			function = LED_FUNCTION_STATUS;
			color = <LED_COLOR_ID_RED>;
			gpios = <&tlmm 1 GPIO_ACTIVE_HIGH>;
			panic-indicator;
		};

		led_status_green: status-green {
			function = LED_FUNCTION_STATUS;
			color = <LED_COLOR_ID_GREEN>;
			gpios = <&tlmm 3 GPIO_ACTIVE_HIGH>;
		};

		led-1 {
			function = LED_FUNCTION_WLAN;
			function-enumerator = <0>;
			color = <LED_COLOR_ID_GREEN>;
			gpios = <&tlmm 22 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "phy0tpt";
		};

		led-2 {
			function = LED_FUNCTION_WLAN;
			function-enumerator = <1>;
			color = <LED_COLOR_ID_GREEN>;
			gpios = <&tlmm 23 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "phy1tpt";
		};

		led-3 {
			function = LED_FUNCTION_WAN;
			color = <LED_COLOR_ID_GREEN>;
			gpios = <&tlmm 28 GPIO_ACTIVE_HIGH>;
		};

		led-4 {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_LAN;
			function-enumerator = <1>;
			gpios = <&tlmm 29 GPIO_ACTIVE_HIGH>;
		};

		led-5 {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_LAN;
			function-enumerator = <2>;
			gpios = <&tlmm 30 GPIO_ACTIVE_HIGH>;
		};

		led-6 {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_LAN;
			function-enumerator = <3>;
			gpios = <&tlmm 31 GPIO_ACTIVE_HIGH>;
		};

		led-7 {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_LAN;
			function-enumerator = <4>;
			gpios = <&tlmm 32 GPIO_ACTIVE_HIGH>;
		};

	};
	output-usb-power {
		compatible = "regulator-output";
		vout-supply = <&reg_usb>;
	};

	reg_usb: regulator-usb {
		compatible = "regulator-fixed";
	
		pinctrl-0 = <&reg_usb_pins>;
		pinctrl-names = "default";
	
		enable-active-high;
		gpios = <&tlmm 44 GPIO_ACTIVE_HIGH>;
		regulator-max-microvolt = <5000000>;
		regulator-min-microvolt = <5000000>;
		regulator-name = "reg_usb";
		regulator-boot-on;
	};

	output-mpcie-power {
		compatible = "regulator-output";
		vout-supply = <&reg_mpcie>;
	};
	
	reg_mpcie: regulator-mpcie {
		compatible = "regulator-fixed";
	
		pinctrl-0 = <&reg_mpcie_pins>;
		pinctrl-names = "default";
	
		enable-active-high;
		gpios = <&tlmm 51 GPIO_ACTIVE_HIGH>;
		regulator-max-microvolt = <5000000>;
		regulator-min-microvolt = <5000000>;
		regulator-name = "reg_mpcie";
		regulator-boot-on;
	};

};

&tlmm {
	serial_pins: serial_pinmux {
		mux {
			pins = "gpio16", "gpio17";
			function = "blsp_uart0";
			bias-disable;
		};
	};

	spi_0_pins: spi_0_pinmux {
		pin {
			function = "blsp_spi0";
			pins = "gpio13", "gpio14", "gpio15";
			drive-strength = <12>;
			bias-disable;
		};

		pin_cs {
			function = "gpio";
			pins = "gpio12";
			drive-strength = <2>;
			bias-disable;
			output-high;
		};
	};
	
	reg_usb_pins: reg_usb_pinmux {
		gpio {
			pins = "gpio44";
			function = "gpio";
			drive-strength = <8>;
			bias-disable;
			output-high;
		};
	};

	reg_mpcie_pins: reg_mpcie_pinmux {
		gpio {
			pins = "gpio51";
			function = "gpio";
			drive-strength = <8>;
			bias-disable;
			output-high;
		};
	};

};

&blsp_dma {
	status = "okay";
};

&blsp1_spi1 {
	status = "okay";

	pinctrl-0 = <&spi_0_pins>;
	pinctrl-names = "default";
	cs-gpios = <&tlmm 12 GPIO_ACTIVE_HIGH>;

	flash@0 {
		reg = <0>;
		compatible = "jedec,spi-nor";
		spi-max-frequency = <10000000>;
		#address-cells = <1>;
		#size-cells = <1>;

		partitions {
			compatible = "fixed-partitions";

			partition@0 {
				label = "Qualcomm";
				reg = <0x0 0x80000>;
				read-only;
			};

			partition@80000 {
				compatible = "mikrotik,routerboot-partitions";
				label = "RouterBoot";
				reg = <0x80000 0x80000>;

				hard_config {
					size = <0x2000>;
					nvmem-layout {
						compatible = "mikrotik,routerboot-nvmem";

						macaddr_hard: base-mac-address {
							#nvmem-cell-cells = <1>;
						};
					};
				};

				dtb_config {
					read-only;
				};

				soft_config {
				};
			};

			partition@110000 {
				compatible = "mikrotik,minor";
				label = "firmware";
				reg = <0x100000 0xf00000>;
			};
		};
	};
};

&blsp1_uart1 {
	status = "okay";
	pinctrl-0 = <&serial_pins>;
	pinctrl-names = "default";
};

&cryptobam {
	status = "okay";
};

&usb3_ss_phy {
	status = "okay";
};

&usb3_hs_phy {
	status = "okay";
};

&usb3 {
	status = "okay";
};

&usb2_hs_phy {
	status = "okay";
};

&usb2 {
	status = "okay";
};

&mdio {
	status = "okay";
};

&gmac {
	status = "okay";
};

&switch {
	status = "okay";
};

&swport1 {
	status = "okay";
	label = "lan4";
	nvmem-cells = <&macaddr_hard 4>;
	nvmem-cell-names = "mac-address";
};

&swport2 {
	status = "okay";
	label = "lan3";
	nvmem-cells = <&macaddr_hard 3>;
	nvmem-cell-names = "mac-address";
};

&swport3 {
	status = "okay";
	label = "lan2";
	nvmem-cells = <&macaddr_hard 2>;
	nvmem-cell-names = "mac-address";
};

&swport4 {
	status = "okay";
	label = "lan1";
	nvmem-cells = <&macaddr_hard 1>;
	nvmem-cell-names = "mac-address";
};

&swport5 {
	status = "okay";
	nvmem-cells = <&macaddr_hard 0>;
	nvmem-cell-names = "mac-address";
};

&wifi0 {
	status = "okay";
};

&wifi1 {
	status = "okay";
};

&crypto {
	status = "okay";
};

&watchdog {
	status = "okay";
};

&prng {
	status = "okay";
};
